{{#bs-button type="primary"
    class="list-group-item list-group-item-action"
    onClick=@onClick
    disabled=(not @enabled)}}
  <quiz-title>{{@quiz.title}}</quiz-title>
  {{bs-tooltip title=(concat @quiz.questions.length " questions") placement="right"}}

  {{#if @answeredQuiz}}
    {{fa-icon "check" class="text-success"}}
  {{else if @enabled}}
    {{fa-icon "chevron-right"}}
  {{/if}}
{{/bs-button}}